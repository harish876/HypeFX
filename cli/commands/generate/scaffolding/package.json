{
  "name": "hypefx",
  "version": "1.0.0",
  "description": "hypefx is a scaffolding tool for a project using htmx, templ and any web framework like echo.",
  "main": "tailwind.config.js",
  "scripts": {
    "start:tailwind": "npx tailwindcss -i ./input.css -o ./assets/css/styles.css --watch",
    "start:go": "air -c ./.air.toml",
    "start": "concurrently \"npm run start:tailwind\" \"npm run start:go\""
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "concurrently": "^8.2.2",
    "daisyui": "^4.7.2",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "preline": "^2.0.3"
  },
  "keywords": [
    "tailwind",
    "htmx",
    "templ",
    "cobra",
    "template"
  ],
  "author": "harish876",
  "license": "ISC"
}
